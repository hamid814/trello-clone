{
  "name": "trello-clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^2.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.1.1",
    "uniqid": "^5.2.0"
  },
  "homepage": "http://hamid814.github.io/trello-clone",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "git add . && git commit -m \"dev\" && npm run build && gh-pages -d build",
    "publish": "gh-pages -d build",
    "git": "git add . && git commit -m \"dev\" && git push -u origin master"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
